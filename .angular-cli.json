        "../src/assets/css/style.default.css",
        "../src/assets/vendor/datatable/datatables.min.css",
        "../src/assets/vendor/datatable/Select-1.2.5/css/select.dataTables.min.css",
        "../src/assets/css/custom.css",
        "../src/assets/vendor/select2/css/select2.min.css",
